<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="App.Routes.ButtonRepoPage"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:buttons="clr-namespace:App.Controls.Buttons;assembly=App"
             xmlns:resx="clr-namespace:App.Resources.Languages;assembly=App"
             Title="Button repo"
             BackgroundColor="{DynamicResource PrimaryBackgroundColour}">
    <VerticalStackLayout Padding="20" VerticalOptions="Center">
        <buttons:CallOut x:Name="repoButton"
                         IsVisible="False"
                         Text="{x:Static resx:AppResources.BtnAddReminderNow}" />

    </VerticalStackLayout>
</ContentPage>